    },
    "yelp_labelled": {
        "data": "datasets/tests/yelp_labelled.txt",
        "metric": "youtube_acc2",
        "finetune_n_train": 250
    },
    "amazon_cells_labelled": {
        "data": "datasets/tests/amazon_cells_labelled.txt",
        "metric": "youtube_acc2",
        "finetune_n_train": 250
    },
    "ss_youtube": {
        "data": "datasets/tests/ss_youtube.pickle",
        "metric": "youtube_acc2",
        "finetune_n_train": 1000
    },
        "ss_twitter": {
       "data": "datasets/tests/ss_twitter.pickle",
        "metric":"youtube_acc2",
