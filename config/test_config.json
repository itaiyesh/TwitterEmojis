{
  "name": "saved_tests",
  "cuda": true,
  "gpu": 0,
      "amazon_review": {
        "data": "datasets/tests/amazon_review_train.csv",
        "test": "datasets/tests/amazon_review_test.csv",
        "metric": "youtube_acc2"
    },

  "test_cases": {

    "imdb_labelled": {
        "data": "datasets/tests/imdb_labelled.txt",
